;TypeFont
FONT=Arial,8,F,#000000,#FFFFFF
;Type
TEXT="1-{(TYPE)}",[TYPE],0%,7.5%,100%,5%,CENTER,CENTER
;CardImage
IMAGE="1-{(IMG)}",[IMG],0%,32.5%,100%,35%
;CleanEffectFont
FONT=Arial,10,F,#000000,#B1EDBB
;CleanEffect
TEXT="1-{(EFFECT CLEAN)}",[EFFECT CLEAN],0%,12.5%,100%,20%,CENTER,CENTER
;BuggyEffectFont
FONT=Arial,10,F,#000000,#FFD6BA
;BuggyEffect
